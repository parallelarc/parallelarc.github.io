ANTHROPIC_COMPAT_API_KEY=your_compatible_anthropic_api_key
# Example for BigModel compatibility:
ANTHROPIC_COMPAT_BASE_URL=https://open.bigmodel.cn/api/anthropic
ANTHROPIC_COMPAT_AUTH_MODE=x-api-key
ANTHROPIC_COMPAT_VERSION=2023-06-01
ANTHROPIC_COMPAT_MODEL=claude-3-5-sonnet-latest

DEFAULT_PROVIDER=anthropic-compatible

# Comma-separated allowlist. Leave empty to allow all origins in local dev.
ALLOWED_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# Optional if you want to test other providers:
# OPENAI_API_KEY=your_openai_api_key
# OPENAI_MODEL=gpt-4o-mini
# ANTHROPIC_API_KEY=your_anthropic_api_key
# ANTHROPIC_MODEL=claude-3-5-sonnet-latest
